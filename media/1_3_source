#include <iostream>

using namespace std;

long long compute(long long x, long long n, long long p) {

	long long res = 1;
	for(long long exp = 1; exp <= n; ++exp) {
		res = ((res % p) * (x % p)) % p; 
	}

	return res;
}

int main() {

	long long x, n, p;
	cout<<"Enter x, n, p: ";
	cin>>x>>n>>p;

	n = n % (p - 1);

	cout<<compute(x, n, p)<<endl;

	return 0;
}